<!doctype html>
<html lang="en">
  <%- include('../includes/head', { title: 'Netlify CMS Example', description: 'This a page created by the millimetr Netlify CMS starter.'}) %>

  <body>
    <nav>
      <ul class="menu">
        <% menu.forEach(menuItem => { %>
          <li>
            <a class="menu-item" href="<%= menuItem.url %>"><%= menuItem.title %></a>
          </li>
        <% }); %>
      </ul>
    </nav>

    <hr />

    <h1><%= title %></h1>
    <%- html %>
  </body>
</html>